@model IEnumerable<VuThiThanh_48.Models.TinhThanh>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Tỉnh Thành</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
           <label>Mã Tỉnh Thành</label>
        </th>
        <th>
            <label> Tên Tỉnh Thành</label>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaTinhThanh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenTinhThanh)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaTinhThanh }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaTinhThanh }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaTinhThanh })
    </td>
</tr>
}

</table>
